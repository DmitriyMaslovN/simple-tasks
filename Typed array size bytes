/*
Int8Array	1
Uint8Array	1
Uint8ClampedArray	1
Int16Array	2
Uint16Array	2
Int32Array	4
Uint32Array	4
Float32Array	4
Float64Array	8
*/

let bufferSizeByte = 64;
let buffer = new ArrayBuffer(bufferSizeByte);
let i32View = new Int32Array(buffer); // each element size 4 byte
console.log(i32View.byteLength); // 64
console.log(i32View.length); // 16
