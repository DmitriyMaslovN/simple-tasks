function LinkedList() { 
  var length = 0; 
  var head = null; 

  var Node = function(element){
    this.element = element; 
    this.next = null; 
  }; 

  this.head = function(){
    return head;
  };

  this.size = function(){
    return length;
  };

  this.add = function(element){

    if(length === 0){
      head = new Node(element);
      length++;
    }else if(head.next === null){ 
      element = new Node(element);
      head.next = element; // linked previous elem
      length++;
    }else if(head.next.next === null){
      element = new Node(element);
      head.next.next = element;
      length++;
    }
  };
}
